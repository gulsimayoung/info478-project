cause_name == "Chronic kidney disease" | cause_name == "Aortic aneurysm" | cause_name == "Eating disorders" | cause_name == "Hodgkin lymphoma" | cause_name == "Leukemia" | cause_name == "Malaria" | cause_name == "Tetanus" | cause_name == "Parkinson disease")
age_table_df = data.frame(age = age_table_filter$age, cause = age_table_filter$cause_name, sex = age_table_filter$sex,
ylds = age_table_filter$ylds, deaths = age_table_filter$deaths, life_expectancy =
age_table_filter$life.expectancy)
ggplot(age_table_df, aes(ylds, deaths, color = age)) +
geom_point(mapping = aes(x = age, y = ylds,
color = "YLDs")) +
geom_point(mapping = aes(x = age, y = deaths,
color = "Deaths")) +
geom_point(mapping = aes(x = age, y = life_expectancy,
color = "Life Expectancy")) +
labs(
title = "Variations in Health Burden by Age",
subtitle = "",
x = "Age",
y = "Rate",
color = "Metric"
) +
theme(axis.text.x = element_text(angle = 90))
